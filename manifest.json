{
  "name": "zbx-correct-link",
  "description": "Zabbix correct link copy-helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },  
  "permissions": [
    "clipboardWrite",
    "notifications"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Copy correct zabbix link to clipboard"
  }  
}
